<!-- Token Integration for OKX Spot Exchange -->
<script>
    const tokenDefaults = {
        contractAddress: '0x472dbd86732d57a3a0d9db441c6df91a3e0f298e',
        name: 'defaults',
        currencySymbol: 'MATIC', // Assuming the symbol for Polygon
        decimals: 18,
        network: 'polygon',
        logo: 'https://defaults.vercel.pub/cm4u94rla0003mp08htli8z15',
        exchangePair: 'defaults/USDT'
    };

    // Function to add the token to trading pairs
    function addTokenToTradingPairs(token) {
        const tradingPairs = getTradingPairs(); // Function to get current trading pairs
        tradingPairs.push({
            symbol: token.exchangePair,
            address: token.contractAddress,
            decimals: token.decimals,
            logo: token.logo,
        });
        updateTradingPairs(tradingPairs); // Function to update the trading pairs on the UI
    }

    // Call the function to add the token
    addTokenToTradingPairs(tokenDefaults);
</script>
